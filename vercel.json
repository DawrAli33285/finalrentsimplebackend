{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/webhook",
        "dest": "index.js",
        "methods": ["POST"]
      },
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "functions": {
      "index.js": {
        "maxDuration": 30,
        "memory": 1024
      }
    }
  }